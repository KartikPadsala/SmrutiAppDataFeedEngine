import javax.ws.rs.Produces;
import javax.ws.rs.core.MediaType;

@Path("/datafeed-engine")
public class SmrutiAppReceiver {

	@GET
	@Produces(MediaType.TEXT_HTML)
	public String sayHtmlHello() {
		return "Hello from Jersey";
	}
}
